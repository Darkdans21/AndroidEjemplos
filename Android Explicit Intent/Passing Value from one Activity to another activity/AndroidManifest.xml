<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.fsw_android.codigomaestro">

    <!-- Aqui debajo de estas lineas se insertaran los permisos necesarios para que funcione nuestra aplicación -->


    <!-- Despues de las librerias, van las clases java, si no se insertan aquí, nunca seran llamados, -->
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity2"></activity>
    </application>

</manifest> <!-- Fin del archivo Android Manifest. -->
